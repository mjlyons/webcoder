{
  "name": "webcoder-client",
  "version": "0.0.1",
  "description": "Front-end for webcoder web-based source editor",
  "main": "index.js",
  "scripts": {
    "build": "webpack -p --progress --color",
    "test": "jest",
    "watch:build": "webpack -d --progress --color --watch",
    "dev": "webpack-dev-server -d --inline --progress --colors"
  },
  "dependencies": {
    "babel-core": "^6.3.26",
    "babel-loader": "^6.2.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "classnames": "^2.2.1",
    "css-loader": "^0.23.1",
    "flux": "^2.1.1",
    "html-webpack-plugin": "^1.7.0",
    "node-sass": "^3.4.2",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "webpack": "^1.12.9"
  },
  "devDependencies": {
    "babel-jest": "^5.3.0",
    "jest-cli": "^0.8.2",
    "jest-webpack-alias": "^2.0.0",
    "react-addons-test-utils": "^0.14.3",
    "webpack-dev-server": "^1.14.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/jestPreprocessor.js",
    "testPathDirs": ["<rootDir>/js"],
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/react",
      "<rootDir>/node_modules/react-dom",
      "<rootDir>/node_modules/react-addons-test-utils",
      "<rootDir>/node_modules/fbjs",
      "<rootDir>/node_modules/classnames",
      "<rootDir>/style"
    ]
  },
  "modulePathIgnorePatterns": [
    "<rootDir>/node_modules/",
    "<rootDir>/style/"
  ],
  "rootDir": "<rootDir>"
}
