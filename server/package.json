{
  "name": "webcoder-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "gulp",
    "start": "npm run build && cd build && node ./bin/www",
    "test": "jest src",
    "lint": "gulp lint",
    "watch:start": "nodemon --ignore build/ --exec 'npm test && npm run lint && npm start'",
    "watch:test": "nodemon --ignore build/ --exec 'npm test && npm run lint'"
  },
  "dependencies": {
    "babel-preset-es2015": "^6.3.13",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "gulp": "^3.9.0",
    "gulp-babel": "^6.1.1",
    "gulp-eslint": "^1.1.1",
    "gulp-if": "^2.0.0",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "babel-jest": "^6.0.1",
    "babel-polyfill": "^6.3.14",
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.11.3",
    "jest-cli": "^0.8.2",
    "nodemon": "^1.8.1",
    "supertest": "^1.1.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/node_modules/babel-jest",
    "testPathDirs": ["<rootDir>/src"],
    "unmockedModulePathPatterns": [
      "debug"
    ]
  }
}
