{
  "name": "webcoder-server",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "gulp",
    "start": "cd build && node ./bin/www",
    "test": "jest --testPathPattern build/",
    "watch:build": "gulp watch",
    "watch:start": "nodemon --delay 5",
    "watch:test": "nodemon --watch build --delay 5 --exec 'npm test'"
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "gulp": "^3.9.0",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "eslint": "^1.10.3",
    "eslint-config-airbnb": "^2.1.1",
    "eslint-plugin-react": "^3.11.3",
    "gulp-babel": "^6.1.1",
    "gulp-eslint": "^1.1.1",
    "gulp-if": "^2.0.0",
    "gulp-jasmine": "^2.2.1",
    "jest-cli": "^0.8.2"
  }
}
